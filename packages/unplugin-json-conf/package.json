{
  "name": "@bluesyoung/unplugin-json-conf",
  "version": "0.0.2",
  "packageManager": "pnpm@7.1.1",
  "description": "在运行/打包过程中，加载指定位置的JSON文件的内容作为虚拟模块的默认导出内容",
  "keywords": [
    "json-conf",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "homepage": "https://github.com/antfu/unplugin-starter#readme",
  "bugs": {
    "url": "https://github.com/antfu/unplugin-starter/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/unplugin-starter.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "test": "vitest",
    "release": "bumpp --commit",
    "push": "pnpm publish --access public"
  },
  "dependencies": {
    "unplugin": "^0.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/node": "^17.0.34",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.0",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.6",
    "webpack": "^5.72.1"
  }
}
